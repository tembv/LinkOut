{
  "manifest_version": 3,
  "name": "LinkOut",
  "description": "Remove Promo Posts",
  "version": "1.1",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/jobs/*"],
      "js": ["js/content.js"]
    }
  ]
}
